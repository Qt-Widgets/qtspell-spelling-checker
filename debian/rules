#!/usr/bin/make -f

export DEB_LDFLAGS_MAINT_APPEND=-Wl,--as-needed

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- 

override_dh_install:
	dh_install --fail-missing

override_dh_strip:
	dh_strip --dbg-package=libqtspell-dbg

.PHONY: override_dh_strip override_dh_auto_configure override_dh_install